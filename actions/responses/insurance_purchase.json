{
  "insurance_purchase": {
    "type": "form",
    "subtype": "Apply Online",
    "action": "rest/v1/user/userInterest",
    "Back": {
      "title": "Back",
      "payload": "menu"
    },
    "title": "Insurance Purchase Form",
    "data": [
      {
        "label": "Full name",
        "placeholder": "Enter full name",
        "id": "fullname",
        "type": "text",
        "validation": {
          "required": true,
          "fullName": true
        }
      },
      {
        "label": "Current Address",
        "placeholder": "Enter your address",
        "id": "current_address",
        "type": "text",
        "validation": {
          "required": true,
          "fullName": true
        }
      },
      {
        "label": "Mobile Number",
        "placeholder": "Enter mobile number",
        "id": "mobileNumber",
        "type": "number",
        "validation": {
          "required": true,
          "mobile": true
        }
      },
      {
        "type": "dropdown",
        "placeholder": "Choose insurance type",
        "label": "Insurance Category",
        "id": "droper",
        "validation": {
          "required": false,
          "droper": true
        },
        "data": [
          {
            "for": "Select Your Category",
            "id": "default"
          },
          {
            "for": "Child Plan"
          },
          {
            "for": "Whole Life Plan"
          },
          {
            "for": "Annual Cash Back Plan"
          },
          {
            "for": "Money Back Plan"
          },
          {
            "for": "Endowment Plan"
          }
        ]
      },
      {
        "label": "Email Address",
        "placeholder": "Enter email address",
        "id": "mobile_email",
        "type": "email",
        "validation": {
          "required": true,
          "email": true
        }
      }
    ],
    "buttons": {
      "data": [
        {
          "text": "Cancel",
          "type": "cancel"
        },
        {
          "text": "Submit",
          "type": "submit"
        }
      ]
    }
  }
}